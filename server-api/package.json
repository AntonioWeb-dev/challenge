{
  "name": "challenge",
  "version": "1.0.0",
  "main": "index.js",
  "license": "MIT",
  "scripts": {
    "proto": "sh proto-gen.sh",
    "compile": "grpc_tools_node_protoc --js_out=import_style=commonjs,binary:./proto --grpc_out=grpc_js:./proto -I ./proto ./proto/*.proto && grpc_tools_node_protoc --plugin=protoc-gen-ts=./node_modules/.bin/protoc-gen-ts --ts_out=grpc_js:./proto -I ./proto ./proto/*.proto",
    "start": "ts-node-dev src/index.ts"
  },
  "dependencies": {
    "@prisma/client": "4.8.0",
    "dotenv": "^16.0.3",
    "express": "^4.18.2",
    "grpc": "^1.24.11",
    "jsonwebtoken": "^9.0.0",
    "module-alias": "^2.2.2",
    "prisma": "^4.8.0",
    "uuid": "^9.0.0"
  },
  "devDependencies": {
    "@grpc/grpc-js": "^1.8.0",
    "@grpc/proto-loader": "^0.7.4",
    "@types/google-protobuf": "^3.15.6",
    "@types/jsonwebtoken": "^9.0.0",
    "@types/module-alias": "^2.0.1",
    "@types/node": "^18.11.17",
    "@types/uuid": "^9.0.0",
    "grpc-tools": "^1.12.3",
    "grpc_tools_node_protoc_ts": "^5.3.2",
    "ts-node-dev": "^2.0.0",
    "typescript": "^4.9.4"
  }
}
